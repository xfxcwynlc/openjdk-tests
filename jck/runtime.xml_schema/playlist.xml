<playlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../TKG/playlist.xsd">
	<include>../jck.mk</include>
	<test>
		<testCaseName>jck-runtime-xml_schema-boeingData</testCaseName>
		<variations>
			<variation>NoOptions</variation>
		</variations>
		<command>$(JCK_CMD_TEMPLATE) -test-args=$(Q)tests=xml_schema/boeingData,jckRoot=$(JCK_ROOT),jckversion=$(JCK_VERSION),testsuite=RUNTIME$(Q); \
	$(TEST_STATUS)</command>
		<levels>
			<level>extended</level>
		</levels>
		<groups>
			<group>jck</group>
		</groups>
	</test>
	<test>
		<testCaseName>jck-runtime-xml_schema-msData</testCaseName>
		<disabled>
			<comment>Disabled on win due to backlog/issues/487. Awaiting some automation improvements, in the interim these targets will be run manually</comment>
			<plat>x86-64_windows(_mixed)?</plat>
			<version>11</version>
		</disabled>
		<variations>
			<variation>NoOptions</variation>
		</variations>
		<command>$(JCK_CMD_TEMPLATE) -test-args=$(Q)tests=xml_schema/msData,jckRoot=$(JCK_ROOT),jckversion=$(JCK_VERSION),testsuite=RUNTIME$(Q); \
	$(TEST_STATUS)</command>
